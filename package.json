        "axios": "^0.21.4",
        "laravel-mix": "^6.0.49",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "postcss-import": "^8.1.2",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.77.5",
        "sass-loader": "^12.1.0",
        "sweetalert2": "^11.11.0"
    },
    "dependencies": {
        "autoprefixer": "^10.4.19",
        "bootstrap": "^5.3.3",
        "toastr": "^2.1.4",
        "typeface": "0.0.0"
    }
